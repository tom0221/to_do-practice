<h1>Lists一覧</h1>
	<h2><%= link_to "新規リスト作成", new_list_path %></h2>
	<% @lists.each do |list| %>
		<p><%= link_to list.name, list_path(list) %></p>
		<%= list.description %>
		<p><%= link_to "編集する", edit_list_path(list) %></p>
		<p><%= link_to "削除する", list_path(list), method: :delete %></p>
	<% end %>


